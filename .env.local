# API Keys para la aplicación de Taxi Ruta
# Reemplaza estos valores con tus API keys reales

# Google Maps API Key (para servicios de mapas y ubicación)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# WebSocket URL para mensajería en tiempo real (opcional)
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:8080

# Base URL de la aplicación
NEXT_PUBLIC_APP_URL=http://localhost:8000

# Configuración de la base de datos (para futuras implementaciones)
DATABASE_URL=your_database_url_here

# Claves para servicios de notificaciones push (opcional)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_vapid_public_key_here
VAPID_PRIVATE_KEY=your_vapid_private_key_here

# Configuración de SMS/WhatsApp para notificaciones (opcional)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
